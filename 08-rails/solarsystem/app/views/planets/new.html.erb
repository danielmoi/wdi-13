<h2>New Planet</h2>

<form action="<%= planets_path %>" method="post">
  <p><label>Name: </label><input type="text" name="name"></p>
  <p><label>Image: </label><input type="url" name="image"></p>
  <p><label>Orbit:</label><input type="number" name="orbit"></p>
  <p><label>Diameter: </label><input type="number" name="diameter"></p>
  <p><label>Mass: </label><input type="number" name="mass"></p>
  <p><label>Moons: </label><input type="number" name="moons"></p>
  <button>Create planet</button>
</form>
